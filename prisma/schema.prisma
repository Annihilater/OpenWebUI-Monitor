generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL_NON_POOLING")
}

model ModelPrice {
  id            String   @id
  name          String
  inputPrice    Float    @default(60)
  outputPrice   Float    @default(60)
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
} 